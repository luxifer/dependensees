    "type": "library",
    "keywords": ["dependencies"],
    "license": "MIT",
    "homepage": "http://luxifer.github.io/dependensees",
    "authors": [
        {
            "name": "Florent Viel",
            "email": "luxifer666@gmail.com",
            "homepage": "http://florentviel.com",
            "role": "Developer"
        }
    ],
    "require": {
