{
    "name": "luxifer/dependensees",
    "description": "Command-line tool to check wether composer dependencies are up to date",
    "version": "1.0.0",
    "type": "library",
    "keywords": ["dependencies"],
    "license": "MIT",
    "authors": [
        {
            "name": "Florent Viel",
            "email": "luxifer666@gmail.com",
            "homepage": "http://florentviel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/console": ">=2.1,<2.4-dev",
        "composer/composer": "1.0.*@dev",
        "guzzle/guzzle": "*",
        "symfony/filesystem": ">=2.1,<2.4-dev",
        "components/bootstrap": "2.3.2",
        "twig/twig": "1.*"
    },
    "autoload": {
        "psr-0": {
            "DependenSees\\": "lib/"
        }
    },
    "bin": ["bin/dependensees"],
    "config": {
        "component-dir": "public/assets"
    }
}
